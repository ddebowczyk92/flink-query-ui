{
    "name": "flink-query-ui",
    "description": "Flink SQL Query Editor react component",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prettier:format": "prettier --write \"./src/**/*.{ts,tsx}\"",
        "prettier:check": "prettier --check \"./src/**/*.{ts,tsx}\"",
        "check": "npm install && npm run lint && npm run prettier:check",
        "check:clean": "npm clean-install && npm run lint && npm run prettier:check",
        "preview": "vite preview"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@monaco-editor/react": "^4.7.0",
        "@mui/icons-material": "^7.3.2",
        "@mui/material": "^7.3.2",
        "@mui/x-data-grid": "^8.14.0",
        "@mui/x-tree-view": "^8.13.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.44.1",
        "@typescript-eslint/parser": "^8.44.1",
        "@vitejs/plugin-react": "^5.0.3",
        "eslint": "^9.36.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.21",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "vite": "^7.1.7"
    }
}
